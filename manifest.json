
{
  "manifest_version": 3,
  "name": "пятый континент",
  "version": "1.0",
  "description": "Добавляет вращение к выбранному HTML элементу",
  "permissions": ["activeTab"],
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "icons": {
    "64": "images/icon64.png",
    "128": "images/icon128.png"
  }
}