{
    "manifest_version": 2,
    "name": "пятый континет",
    "version": "1.0",
    "description": "Добавляет вращение к выбранному HTML элементу",
    "permissions": ["activeTab"],
    "browser_action": {
      "default_popup": "popup.html"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ] 
  }